<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pais.home.view.HomeActivity"
    tools:showIn="@layout/app_bar_main">

    <TextView
        android:text="오늘 2016/07/22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/content_main"
        android:layout_marginTop="16dp" />

    <TextView
        android:text="측정시간 09:00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/content_main"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="89%"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sub_sensor_data"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:text="09:00  27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/set_time_1st"
        app:layout_constraintLeft_toRightOf="@+id/main_sensor_data"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="15:00  32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/set_time_2nd"
        app:layout_constraintLeft_toRightOf="@+id/main_sensor_data"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/set_time_1st"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/main_sensor_data"
        android:textSize="50sp"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_temperature"
        android:id="@+id/imageView2"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginTop="24dp" />

    <ImageView
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ic_humidity"
        android:id="@+id/imageView3"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:layout_marginTop="32dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/content_main"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/content_main"
        android:layout_marginBottom="16dp"
        app:layout_constraintHorizontal_bias="0.33"
        app:layout_constraintVertical_bias="0.35000002" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        tools:layout_editor_absoluteX="310dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        app:layout_constraintLeft_toLeftOf="@+id/content_main"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintRight_toRightOf="@+id/content_main"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.67" />

</android.support.constraint.ConstraintLayout>
